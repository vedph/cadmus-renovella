<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <Authors>Daniele Fusi</Authors>
    <Company>Fusi</Company>
    <Product>Cadmus</Product>
    <Description>Cadmus components for the Re.Novella project.</Description>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Version>2.0.1</Version>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <Copyright>by Daniele Fusi</Copyright>
    <PackageTags>Cadmus;Re.Novella parts</PackageTags>
    <FileVersion>2.0.1</FileVersion>
    <AssemblyVersion>2.0.1</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Cadmus.Core" Version="3.0.0" />
    <PackageReference Include="Cadmus.General.Parts" Version="1.0.4" />
    <PackageReference Include="Cadmus.Refs.Bricks" Version="3.0.0" />
  </ItemGroup>
</Project>
