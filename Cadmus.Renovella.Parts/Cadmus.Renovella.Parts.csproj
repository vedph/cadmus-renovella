<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<Nullable>enable</Nullable>
    <TargetFramework>net7.0</TargetFramework>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <Authors>Daniele Fusi</Authors>
    <Company>Fusi</Company>
    <Product>Cadmus</Product>
    <Description>Cadmus components for the Re.Novella project.</Description>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Version>4.0.1</Version>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <Copyright>by Daniele Fusi</Copyright>
    <PackageTags>Cadmus;Re.Novella parts</PackageTags>
    <FileVersion>4.0.1</FileVersion>
    <AssemblyVersion>4.0.1</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Cadmus.Core" Version="6.0.3" />
    <PackageReference Include="Cadmus.General.Parts" Version="4.2.1" />
    <PackageReference Include="Cadmus.Refs.Bricks" Version="4.0.0" />
  </ItemGroup>
</Project>
